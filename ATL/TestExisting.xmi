<?xml version="1.0" encoding="UTF-8"?>
<erdMetamodel:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:erdMetamodel="http://www.example.org/erdMetamodel"
    name="LectureDB">
  <Entity name="LectureSchedule">
    <attributes
        name="ScheduleID"
        isPrimaryKey="true"/>
    <attributes
        name="LectureHallID"/>
    <attributes
        name="LectureID"/>
    <attributes
        name="Starttime"/>
    <attributes
        name="Duration"/>
    <attributes
        name="LecturerID"/>
  </Entity>
  <Entity name="LectureHall">
    <attributes
        name="IDLectureHall"/>
    <attributes
        name="Name"
        isPrimaryKey="true"
        dataType="VARCHAR"/>
    <attributes
        name="Capacity"/>
  </Entity>
  <Entity name="Lecture">
    <attributes
        name="IDLecture"
        isPrimaryKey="true"/>
    <attributes
        name="NameLecture"
        dataType="VARCHAR"/>
    <attributes
        name="Description"
        dataType="VARCHAR"/>
    <attributes
        name="DurationLecture"/>
    <attributes
        name="MainLecturerID"/>
    <attributes
        name="SubjectIDLecture"/>
  </Entity>
  <Entity name="Subject">
    <attributes
        name="SubjectID"
        isPrimaryKey="true"/>
    <attributes
        name="SubjectName"
        dataType="VARCHAR"/>
  </Entity>
  <Entity name="LectureRegistration">
    <attributes
        name="LectureScheduleID"
        isPrimaryKey="true"/>
    <attributes
        name="StudentIDRegistration"/>
  </Entity>
  <Entity name="Student">
    <attributes
        name="StudentID"
        isPrimaryKey="true"/>
    <attributes
        name="Firstname"
        dataType="VARCHAR"/>
    <attributes
        name="Lastname"
        dataType="VARCHAR"/>
    <attributes
        name="StudentCardNumber"
        dataType="VARCHAR"/>
  </Entity>
  <Entity name="Lecturer">
    <attributes
        name="IDLecturer"
        isPrimaryKey="true"/>
    <attributes
        name="Firstname"
        dataType="VARCHAR"/>
    <attributes
        name="Lastname"
        dataType="VARCHAR"/>
    <attributes
        name="Bio"
        dataType="VARCHAR"/>
  </Entity>
  <relationships
      name="HallRelationship"
      target="//@Entity.0/@attributes.2"
      targetTable="//@Entity.0"
      source="//@Entity.1/@attributes.0"
      sourceTable="//@Entity.1"
      type="OneToMany"/>
  <relationships
      name="LectureIDRelationship"
      target="//@Entity.0/@attributes.2"
      targetTable="//@Entity.0"
      source="//@Entity.2/@attributes.0"
      sourceTable="//@Entity.2"
      type="OneToMany"/>
  <relationships
      name="RegistrationLectureRelationship"
      target="//@Entity.4/@attributes.0"
      targetTable="//@Entity.4"
      source="//@Entity.0/@attributes.0"
      sourceTable="//@Entity.0"
      type="OneToMany"/>
  <relationships
      name="StudentRegistrationRelationship"
      target="//@Entity.4/@attributes.1"
      targetTable="//@Entity.4"
      source="//@Entity.5/@attributes.0"
      sourceTable="//@Entity.5"
      type="OneToMany"/>
  <relationships
      name="LectureScheduleRelationship"
      target="//@Entity.0/@attributes.5"
      targetTable="//@Entity.0"
      source="//@Entity.6/@attributes.0"
      sourceTable="//@Entity.6"
      type="OneToMany"/>
  <relationships
      name="LecturerLectureRelationship"
      target="//@Entity.2/@attributes.4"
      targetTable="//@Entity.2"
      source="//@Entity.6/@attributes.0"
      sourceTable="//@Entity.6"
      type="OneToMany"/>
</erdMetamodel:Model>
